<ul class="thumbnails lab-list">
  {{#each labs}}
    <li>
      <div class="thumbnail">
        <a href="#/labs/{{ id }}/steps/1" data-ami="{{ ami }}" data-volume="{{ dataVolume}}">
          <div class="lab-image" style="background-image: url({{#if image}}{{ image }}{{else}}http://placehold.it/300x200{{/if}})"></div>
        </a>
        <div class="caption">
          <h3>{{ name }}</h3>
          <p>{{ description }}</p>
          <a href="#/labs/{{ id }}/steps/1" class="btn btn-primary" data-ami="{{ ami }}" data-volume="{{ dataVolume}}">
            Start Lab
          </a>
        </div>
      </div>
    </li>
  {{/each}}
</ul>